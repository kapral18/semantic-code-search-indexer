{
  "name": "code-indexer",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "index": "NODE_OPTIONS=--max-old-space-size=8192 ts-node src/index.ts index",
    "search": "ts-node src/index.ts search",
    "references": "ts-node src/index.ts references",
    "incremental-index": "ts-node src/index.ts incremental-index",
    "setup": "ts-node src/index.ts setup",
    "start:webhook": "node dist/webhook/server.js",
    "start:local-worker": "node dist/webhook/local_worker.js",
    "start:vm-worker": "node dist/webhook/vm_worker.js",
    "index-and-search": "npm run index"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "@google-cloud/pubsub": "^5.2.0",
    "@types/cli-progress": "^3.11.6",
    "@xenova/transformers": "^2.17.2",
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "ignore": "^7.0.5",
    "p-queue": "^8.1.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-typescript": "^0.23.2",
    "vscode-languageserver-protocol": "^3.17.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
